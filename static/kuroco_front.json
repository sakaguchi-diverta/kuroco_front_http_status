{
    "rewrites": [
        {
          "source": "(?!/404/).*",
          "destination": "/index.html"
        },
        {
          "source": "/404/",
          "destination": "/HTTP_STATUS/non_existing_file.html"
        }
    ],
    "redirects": [
      {
        "source": "^/HTTP_STATUS/.*",
        "destination": "/HTTP_STATUS/non_existing_file.html"
      }
    ],
    "redirects_by_ie": [],
    "basic":[],
    "ip_restrictions":[],
    "stale_while_revalidate":"86400",
    "error_page": {
        "status404":"/HTTP_STATUS/404.html"
    },
    "inject_data": []
}
