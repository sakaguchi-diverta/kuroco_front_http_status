{
    "rewrites": [
        {
          "source": ".*",
          "destination": "/index.html"
        }
    ],
    "redirects": [
        {
          "source": "^/404/",
          "destination": "status:404"
        },
        {
          "source": "^/ERROR/.*",
          "destination": "status:404"
        },
        {
          "source": "^/before1/",
          "destination": "/after1/"
        },
        {
          "source": "^/before2/.*",
          "destination": "/after2/"
        }
    ],
    "redirects_by_ie": [],
    "proxies": [
      {
        "source": "/files/k/v=([0-9]+)/sitemaps/(.*)",
        "destination": "https://sakaguchi.g.kuroco-img.app/v$1/files/user/sitemaps/$2",
        "max_age": 0
      },
      {
        "source": "/files/k/sitemaps/(.*)",
        "destination": "https://sakaguchi.g.kuroco-img.app/files/user/sitemaps/$1",
        "max_age": 0
      }
    ],
    "basic":[],
    "ip_restrictions":[],
    "stale_while_revalidate":"86400",
    "error_page": {
        "status404":"/index.html"
    },
    "inject_data": []
}
