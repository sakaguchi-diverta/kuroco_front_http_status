{
    "rewrites": [
        {
          "source": ".*",
          "destination": "/index.html"
        }
    ],
    "redirects": [
        {
          "source": "^/404/",
          "destination": "status:404"
        },
        {
          "source": "^/ERROR/.*",
          "destination": "status:404"
        }
    ],
    "redirects_by_ie": [],
    "proxies": [
      {
        "source": "/files/sitemaps/(.+\.xml)",
        "destination": "https://sakaguchi.g.kuroco-img.app/files/user/sitemaps/$1"
      }
    ],
    "basic":[],
    "ip_restrictions":[],
    "stale_while_revalidate":"86400",
    "error_page": {
        "status404":"/index.html"
    },
    "inject_data": []
}
