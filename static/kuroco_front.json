{
    "rewrites": [
        {
          "source": "/404/",
          "destination": "/non_existing_file.html"
        },
        {
          "source": "^/ERROR/.*",
          "destination": "/non_existing_file.html"
        },
        {
          "source": ".*",
          "destination": "/index.html"
        }
    ],
    "redirects": [],
    "redirects_by_ie": [],
    "basic":[],
    "ip_restrictions":[],
    "stale_while_revalidate":"86400",
    "error_page": {
        "status404":"/ERROR/404.html"
    },
    "inject_data": []
}
